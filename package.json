  "description": "FluentUI v9 React TypeScript sample with tabbed layout and input controls",
  "main": "src/index.tsx",
  "scripts": {
    "start": "webpack serve --mode development",
    "start:debug": "webpack serve --mode development --devtool eval-source-map",
