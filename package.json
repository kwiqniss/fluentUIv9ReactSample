    "@types/react-router-dom": "^5.3.3",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "xml2js": "^0.6.2"
